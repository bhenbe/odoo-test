    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-qunit": "^7.0.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "find-unused-sass-variables": "^3.1.0",
    "glob": "^7.2.0",
